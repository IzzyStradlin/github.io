    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#Country">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <rdfs:domain rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#q-location-of"/>
        <rdfs:comment>Country of the address.</rdfs:comment>
    </rdf:Description>

