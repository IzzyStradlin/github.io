<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rfilop="https://izzystradlin.github.io/ontologies/rfilop/MasterOntology"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:dolce="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#">

    <owl:Ontology rdf:about="https://izzystradlin.github.io/ontologies/rfilop/MasterOntology"/>

    <!-- Involved Party Class Definition -->
    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#non-physical-endurant"/>
        <rdfs:label>InvolvedParty</rdfs:label>
        <rdfs:comment>An involved party in the logistics process (e.g., shipper, carrier).</rdfs:comment>
    </rdf:Description>

    <!-- InvPartyNumber Property -->
    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#InvPartyNumber">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <rdfs:domain rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-quality"/>
        <rdfs:comment>Unique identifier of the involved party.</rdfs:comment>
    </rdf:Description>

    <!-- InvPartyRole Property -->
    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#InvPartyRole">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <rdfs:domain rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-quality"/>
        <rdfs:comment>Role of the involved party (e.g., Shipper, Carrier).</rdfs:comment>
    </rdf:Description>

    <!-- Name Property -->
    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#Name">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
        <rdfs:domain rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:subPropertyOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-quality"/>
        <rdfs:comment>Name of the company or individual.</rdfs:comment>
    </rdf:Description>

    <!-- ContactPerson Property -->
    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#ContactPerson">
