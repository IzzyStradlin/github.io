        <rdfs:label>Party 1</rdfs:label>
        <rdfs:comment>The main party associated with the sales order.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#Party2">
        <rdf:type rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:label>Party 2</rdfs:label>
        <rdfs:comment>An involved party in Transport Order 1.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#Party3">
        <rdf:type rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:label>Party 3</rdfs:label>
        <rdfs:comment>An involved party in Transport Order 1.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#Party4">
        <rdf:type rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:label>Party 4</rdfs:label>
        <rdfs:comment>An involved party in Transport Order 2.</rdfs:comment>
    </rdf:Description>

    <rdf:Description rdf:about="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty#Party5">
        <rdf:type rdf:resource="https://izzystradlin.github.io/ontologies/rfilop/InvolvedParty"/>
        <rdfs:label>Party 5</rdfs:label>
        <rdfs:comment>An involved party in Transport Order 2.</rdfs:comment>
    </rdf:Description>

</rdf:RDF>